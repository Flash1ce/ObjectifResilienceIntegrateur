      //    "AttachStackTrace": true,
      //    "Debug": true,
      //    "DiagnosticsLevel": "Error",
      //    "Environment": "Development"
      //  }
      //}
    ],
    "Properties": {
      "Application": "Gwenael"
    }
  },
  "Mailing": {
    "FromAddress": "support@spektrummedia.com",
    "FromName": "Gwenael",
    "AwsSes": {
      "AccessKeyId": "SPK_AwsSesAccessKeyId",
      "AccessKeySecret": "SPK_AwsSesAccessKeySecret"
    }
  }
}
