    <link rel="stylesheet" href="~/css/site.css" />

    <environment exclude="Production">
        <meta name="robots" content="noindex">
    </environment>
</head>

<body>

    <div class="globalContainer page">




        <nav class="navbar navbar-expand-lg navbar-light" style="background-color:#ffa733;">
            <div class="container-fluid">
                <a class="navbar-brand" asp-page="/Index">
                    <img src="../images/ObjectifResilience.png" alt="" width="100" height="50" class="d-inline-block align-text-top">
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                    <div class="navbar-nav">
                        <a class="nav-link" href="/indexArticles">Articles</a>
                        <a class="nav-link" href="/Tutoriel">Tutoriels</a>
                        <a class="nav-link" href="/AudioFolder/PoadcastPage">Podcast</a>
                        @foreach (var nouvellePage in ViewData["newPages"] as IList<NewPage>)
                        {
                            if (nouvellePage != null)
                            {
                    <a class="nav-link" href="/NewPage?Titre=@nouvellePage.Titre">@nouvellePage.Titre</a>}
                                }
                        @if (User.Identity.IsAuthenticated)
                        {
                    <a class="nav-link" href="/AdminMenu">Administrateur</a> } @*<span class="navbar-text">*@
                        @await Html.PartialAsync("_LoginPartial")
                        @*</span>*@
                        <!-- Je sais pas comment cétait, faq j'ai juste copier coller ton truc.' -->



                    </div>
                </div>
            </div>
        </nav>

        <div class="body-content main affichageEtendu containerMain z-index -2">
            @RenderBody()
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="#ffa733" fill-opacity="1" d="M0,224L40,234.7C80,245,160,267,240,250.7C320,235,400,181,480,176C560,171,640,213,720,224C800,235,880,213,960,208C1040,203,1120,213,1200,224C1280,235,1360,245,1400,250.7L1440,256L1440,320L1400,320C1360,320,1280,320,1200,320C1120,320,1040,320,960,320C880,320,800,320,720,320C640,320,560,320,480,320C400,320,320,320,240,320C160,320,80,320,40,320L0,320Z"></path></svg>
            <footer>
                <div class="footer">
                    <div class="footer-part1" style="display:flex;flex-direction:column;">
                        <div style="display:flex;justify-content:space-around;">
                            <div>
                                <h3>Où nous trouver</h3>
                                <p style="text-align:center;">200 Adresse a Quebec QC</p>
                            </div>

                            <div>
                                <h3>Nos informations</h3>
                                <p style="margin:0;text-align:center;">Anthony Dev - Abraham Dev</p>
                                <p style="margin:0;text-align:center;">Antoine Dev - Gwenael Dev</p>
                            </div>
                            <div>
                                <h3 style="text-align:left;">Nos coordonnées</h3>
                                <p style="text-align:center;margin:0">Mail: Projet@gmail.com</p>
                                <p style="margin:0;text-align:center;">Tel: 418-000-0000</p>
                            </div>
                        </div>
                        @*                        <div>
