{
	"env": { "browser": true },
	"extends": "standard",
	"rules": {
		"space-before-function-paren": 0,
		"object-property-newline": 0,
		"arrow-parens": 0,
		"new-cap": 0,
		"no-eval": 0,
		"semi": 0
	},
	"globals": {
		"$": false,
		"scriptRangeeInit": false,
		"scriptRangeeUtils": false,
		"window": false,
		"document": false,
		"localStorage": false
	}
}