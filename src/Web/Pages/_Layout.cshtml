<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gwenael</title>
    <link rel="stylesheet" href="~/css/site.css" />
    

    <environment exclude="Production">
        <meta name="robots" content="noindex">
    </environment>
</head>
<body>
    <div class="globalContainer page">
        <!-- En-tête de la page, avec la bannière -->
        
        <header class="affichageEtendu">
            <!-- div pour le flexbox row (en ligne) -->

            
            
            <!-- <h1>Équipe AAAG</h1> -->
        </header>



                <nav class="navbar navbar-expand-lg navbar-dark affichageEtendu z-index 1" id="containerNav">
                    <div class="containerNavLv2">
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-main" aria-controls="navbar-main" aria-expanded="false" aria-label="Toggle navigation" style="">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div><a asp-page="/Index" class="navbar-brand borderCancell"><img src="../images/CJC-ObjectifResilience-Mangue.png" alt="image du site" title="image du site" /></a></div>
                        <div class="collapse navbar-collapse" id="navbar-main">
                            
                            <ul>
                                <li class="nav-item main-nav-item">
                                    <a class="nav-link" href="#">Menu 1</a>
                                </li>
                                <li class="nav-item main-nav-item">
                                    <a class="nav-link" href="#">Menu 2</a>
                                </li>
                                <li class="nav-item main-nav-item">
                                    <a class="nav-link" href="#">Menu 3</a>
                                </li>
                                <li class="nav-item main-nav-item">
                                    <a class="nav-link" href="#">Menu 4</a>
                                </li>
                                <li class="nav-item main-nav-item">
                                    <a class="nav-link" href="#">Menu 5</a>
                                </li>

                            </ul>

                        </div>
                        <div> @await Html.PartialAsync("_LoginPartial") </div>
                    </div>
                </nav>


            <div class="body-content main affichageEtendu containerMain z-index -2">
                @RenderBody()
                <footer>
                    <hr />
                    <p>&copy; @DateTime.Now.Year - Gwenael</p>
                </footer>
            </div>


        </div>
        @* Useless? delete this? *@
